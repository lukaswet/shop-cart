<h2>My account</h2>

  <%= form_for @user do |f| %>

    <div class="form-group">
      <%= f.label :first_name %>
      <%= f.text_field :first_name %>
    </div>
    <div class="form-group">
      <%= f.label :last_name %>
      <%= f.text_field :last_name %>
    </div>
    <%= f.submit 'Save', class: 'btn btn-default' %>

  <% end %>
